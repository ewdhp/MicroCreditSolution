{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-test",
            "program": "{your path}/bin/Debug/net9.0/MicroCredit.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole",
            "logging": {
                "logging.diagnosticsLog.protocolMessages": false
            },
            "env": {
                "Jwt__Key": "YourSecretKeyHere1234567890!@#$%^&*()_+ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz",
                "Jwt__Issuer": "YourIssuerHere",
                "Twilio__AccountSid": "",
                "Twilio__AuthToken": "",
                "Twilio__ServiceSid": ""
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            },
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}
